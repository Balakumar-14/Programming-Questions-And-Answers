#include<stdio.h>
int main()
{
	int a[]={16,17,3,4,5,2};
	int c[10];
	int i,j,n,k;
	n=sizeof(a)/sizeof(a[0]);
	for(i=0;i<n;i++)
	{
		int b[10]={0},m=0,max=0,c1;
		for(j=i+1;j<n;j++)
		{
			b[m]=a[j];
			m++;
		}
		c1=sizeof(b)/sizeof(b[0]);
		max=b[0];
		for(k=0;k<c1;k++)
		{
			if(b[k]>max)
			{
				max=b[k];
			}
		}
		c[i]=max;
		if(i==n-1)
		{
			c[i]=-1;
		}
	}
	for(i=0;i<n;i++)
	{
		printf("%d ",c[i]);
	}
}

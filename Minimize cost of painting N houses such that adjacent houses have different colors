#include <stdio.h>
 
// This function prints common elements in ar1
void FindMinpaintcost(int arr[][3], int n)
{
	int i,j,temp,min,sum=0;
	int a[3];	
	for(i=0;i<n;i++)
	{
		int min=arr[i][0];
		for(j=0;j<n;j++)
		{
			if(arr[i][j]<min && a[i-1]!=j)
			{
				temp=j;
				min=arr[i][j];
			}
		}
		a[i]=j;
		sum+=min;
	}
	printf("%d",sum);
}
 
// Driver code
int main()
{
int arr[][3] = { { 1, 2, 11 },{ 11, 14, 5 },{ 14, 3, 10 } };
int n = sizeof(arr) / sizeof(arr[0]);
FindMinpaintcost(arr, n);
return 0;
}

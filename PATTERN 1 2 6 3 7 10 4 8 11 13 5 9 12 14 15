#include<stdio.h>
int main()
{
	int i,j,t,n=5;
	for(i=1;i<=n;i++)
	{
		int val=i;
		t=n-1;
		for(j=1;j<=i;j++)
		{
			printf("%d ",val);
			val+=t;
			t--;
		}
		printf("\n");
	 } 
	
	return 0;
}


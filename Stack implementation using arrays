#include<stdio.h>
int a[10],top,x,n=10;
int main()
{
	top=-1;
	push(a);
	push(a);
	push(a);
	pop(a);
	display(a);
	return 0;
}
void push(int a[])
{
	printf("Enter the number to be pushed:\n");
	scanf("%d",&x);
	if(top>=n-1)
	{
		printf("Stack overflow");
	}
	else
	{
		top++;
		a[top]=x;
	}
}
void pop(int a[])
{
	top--;
}
void  display(int a[])
{
	int i;
	printf("Elements are:");
	for(i=0;i<=top;i++)
	{
		printf("%d",a[i]);
	}
}

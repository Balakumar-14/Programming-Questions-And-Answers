#include <stdio.h>
 
// This function prints common elements in ar1
void findCommon(int a[], int n)
{
    int counti=1,countd=1,i,j;
    for(i=0;i<n;i++)
    {
    	for(j=i+1;j<n;j++)
    	{
    		if(a[i]>a[j])
    		{
    			int temp;
    			temp=a[i];
    			a[i]=a[j];
    			a[j]=temp;
			}
		}
	}
	int x=a[0],y=a[0];
    for(i=0;i<n;i++)
    {
    	printf("x=%d ,a[%d]=%d\n",x,i,a[i]);
    	if(a[i]==x-1)
    	{
    		x=a[i];
    		countd++;
    		printf("\ncountd-%d\n",countd);
		}
		if(a[i]==y+1)
		{
			y=a[i];
			counti++;
			printf("\ncounti-%d\n",counti);
		}
	}
	(counti>countd)?printf("%d",counti):printf("%d",countd);
}
 
// Driver code
int main()
{
    int ar1[] = {36, 41, 56, 35, 44, 33, 34, 92, 43, 32, 42};
    int n1 = sizeof(ar1)/sizeof(ar1[0]);
 
    findCommon(ar1, n1);
    return 0;
}

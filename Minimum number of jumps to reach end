#include<stdio.h>
#include<stdlib.h>
int minjump(int a[],int n)
{
	if(n<=1)
	{
		return 0;
	}
	int ladder=a[0];
	int stairs=a[0];
	int i,jump=0;
	for(i=0;i<n;i++)
	{
		if(i==n-1)
		{
			return jump;
		}
		if(i+a[i]>ladder)
		{
			ladder=i+a[i];
		}
		stairs--;
		if(stairs==0)
		{
			printf("%d",a[i]);
			jump++;
			stairs=ladder-i;
		}
	}
	return jump;
}
int main()
{
	int a[]={1,2,3,4,5,6,7,8,9};
	int n=sizeof(a)/sizeof(a[0]);
	printf("%d",minjump(a,n));
	return 0;
	
}
